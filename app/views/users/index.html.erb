<% provide(:title, 'All users') %>
<h1>All users</h1>
<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li><%= link_to user.email, edit_user_registration_path(user) %>
    <% if can? :destroy, user %>  
| <%= link_to "delete", user_registration_path(user), :method => :delete, :confirm => "You sure?" %>
 <%end%>  
</li>
  <% end %>
</ul>
<%= will_paginate %>
