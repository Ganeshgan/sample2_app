<% provide(:title, 'All users') %>
<h1>All users</h1>
<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li><%= user.email %>
    | <%= link_to "delete", user_registration_path(user), :method => :delete, :confirm => "You sure?" %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
